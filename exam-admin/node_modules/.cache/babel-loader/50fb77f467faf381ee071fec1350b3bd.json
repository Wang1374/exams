rage.removeItem(\"token\");\n        //清除session\n        sessionStorage.removeItem(\"phone\");\n        //跳转登录页面\n        this.$router.push(\"/\");\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.el-menu-vertical-demo:not(.el-menu--collapse) {\n  width: 230px;\n  min-height: 400px;\n}\n</style>"],"sourceRoot":""}]}