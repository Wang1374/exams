;;AACA,YAAI,EAAA,GAAK,CAAC,IAAG,GAAI,GAAR,IAAa,EAAd,GAAoB,EAA5B;;AACA,YAAI,CAAA,GAAI,QAAQ,CAAC,IAAG,GAAI,IAAR,CAAhB;;AACA,YAAI,CAAA,GAAI,EAAR,EAAY;AACV,UAAA,CAAA,GAAI,MAAM,CAAV;AACF;;AACA,YAAI,EAAA,GAAI,EAAR,EAAY;AACV,UAAA,EAAA,GAAI,MAAM,EAAV;AACF;;AACA,YAAI,GAAA,GAAI,EAAR,EAAY;AACV,UAAA,GAAA,GAAI,MAAM,GAAV;AACF;;AACA,QAAA,KAAI,GAAI,CAAA,GAAI,GAAJ,GAAU,EAAV,GAAc,GAAd,GAAoB,GAA5B;AACF;;AACA,aAAO,KAAP;AACD,KAjHM;AAkHP,IAAA,QAlHO,sBAkHI;AACT;AACA,WAAK,eAAL,GAAuB,EAAvB,CAFS,CAGT;;AACA,UAAM,IAAG,GAAI,EAAb;AACA,WAAK,SAAL,GAAiB,EAAjB;AACA,WAAK,SAAL,GAAiB,KAAK,UAAtB;;AACA,UAAI,KAAK,KAAL,IAAc,EAAlB,EAAsB;AACpB,aAAK,IAAI,CAAA,GAAI,CAAb,EAAgB,CAAA,GAAI,KAAK,SAAL,CAAe,MAAnC,EAA2C,CAAC,EAA5C,EAAgD;AAC9C,cAAI,KAAK,SAAL,CAAe,CAAf,EAAkB,SAAlB,IAA+B,KAAK,KAAxC,EAA+C;AAC7C,YAAA,IAAI,CAAC,IAAL,CAAU,KAAK,SAAL,CAAe,CAAf,CAAV;AACF;AACF;;AACA,aAAK,SAAL,GAAiB,IAAjB;AACF;AACD,KAjIM;AAkIP,IAAA,UAlIO,wBAkIM;AACX;AACA,WAAK,KAAL,GAAa,EAAb,CAFW,CAGX;;AACA,UAAM,IAAG,GAAI,EAAb;AACA,WAAK,SAAL,GAAiB,EAAjB;AACA,WAAK,SAAL,GAAiB,KAAK,UAAtB;;AACA,UACE,KAAK,eAAL,KAAyB,CAAzB,IACA,KAAK,eAAL,KAAyB,CADzB,IAEA,KAAK,eAAL,KAAyB,CAH3B,EAIE;AACA,aAAK,IAAI,CAAA,GAAI,CAAb,EAAgB,CAAA,GAAI,KAAK,SAAL,CAAe,MAAnC,EAA2C,CAAC,EAA5C,EAAgD;AAC9C,cAAI,KAAK,SAAL,CAAe,CAAf,EAAkB,UAAlB,KAAiC,KAAK,eAA1C,EAA2D;AACzD,YAAA,IAAI,CAAC,IAAL,CAAU,KAAK,SAAL,CAAe,CAAf,CAAV;AACF;AACF;;AACA,aAAK,SAAL,GAAiB,IAAjB;AACF;AACD;AArJM;AArEI,CAAf","sourcesContent":["<template>\n  <div style=\"padding:10px;\" class=\"kd\">\n    <div style=\"margin: 10px 0\">\n      <!--功能区域-->\n      <el-row>\n        <el-col :span=\"4\" style=\"max-width: 211px\">\n          <el-input v-model=\"search\" placeholder=\"请输入试卷名称\" style=\"width: 200px;margin-left: 5px\" clearable></el-input>\n        </el-col>\n        <el-col :span=\"3\">\n          <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"load\" icon=\"el-icon-search\">查询</el-button>\n        </el-col>\n        <el-col :span=\"2\" style=\"margin-top: 8px;max-width: 80px;\">试卷类型:</el-col>\n        <el-col :span=\"5\">\n          <el-select v-model=\"value\" placeholder=\"请选择\" @change=\"examType()\">\n            <el-option v-for=\"item in options\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\">\n            </el-option>\n          </el-select>\n        </el-col>\n        <el-col :span=\"2\" style=\"margin-top: 8px;max-width: 80px;\">试卷状态:</el-col>\n        <el-col :span=\"4\">\n          <el-select v-model=\"examStatusValue\" placeholder=\"请选择\" @change=\"examStatus()\">\n            <el-option v-for=\"item in examStatusOptions\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\">\n            </el-option>\n          </el-select>\n        </el-col>\n      </el-row>\n    </div>\n    <el-table :data=\"tableData\" border stripe style=\"width: 99%\">\n      <el-table-column :width=\"cloumWidth.id\" label=\"序号\" type=\"index\" show-overflow-tooltip>\n      </el-table-column>\n      <el-table-column prop=\"id\" label=\"ID\" v-if=\"false\">\n      </el-table-column>\n      <el-table-column prop=\"examName\" label=\"试卷名称\" :show-overflow-tooltip=\"true\" :width=\"cloumWidth.examName\">\n      </el-table-column>\n      <el-table-column prop=\"answerTime\" label=\"考试时间\" :width=\"cloumWidth.AnswerTime\">\n      </el-table-column>\n      <el-table-column prop=\"subjectName\" label=\"试卷类型\" :width=\"cloumWidth.subjectName\">\n      </el-table-column>\n      <el-table-column prop=\"examScore\" label=\"总分\" :width=\"cloumWidth.examScore\">\n      </el-table-column>\n      <el-table-column prop=\"passingScore\" label=\"及格分\" :width=\"cloumWidth.passingScore\">\n      </el-table-column>\n      <el-table-column prop=\"grade\" label=\"得分\" :width=\"cloumWidth.grade\">\n      </el-table-column>\n      <el-table-column prop=\"examHsTime\" label=\"答题耗时\" :width=\"cloumWidth.examHsTime\">\n      </el-table-column>\n      <el-table-column prop=\"topicNum\" label=\"题目数量\" :width=\"cloumWidth.topicNum\">\n      </el-table-column>\n      <el-table-column prop=\"answerDate\" label=\"提交时间\" :width=\"cloumWidth.answerDate\">\n      </el-table-column>\n      <el-table-column prop=\"markDate\" label=\"批改时间\" :width=\"cloumWidth.markDate\">\n      </el-table-column>\n      <el-table-column prop=\"examStatus\" label=\"试卷状态\" :width=\"cloumWidth.examStatus\">\n        <template v-slot=\"scope\">\n          <el-tag v-if=\"scope.row.examStatus===0\" size=\"medium\" type=\"danger\">未完成</el-tag>\n          <el-tag v-else-if=\"scope.row.examStatus===1\" size=\"medium\" type=\"\">批改中</el-tag>\n          <el-tag v-else size=\"medium\" type=\"success\">已完成</el-tag>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"操作\" :width=\"cloumWidth.cz\">\n        <template #default=\"scope\">\n          <el-button size=\"mini\" v-if=\"scope.row.examStatus===0\" type=\"success\" @click=\"handleEdit(scope.row.id,scope.row.examStatus)\" icon=\"el-icon-s-promotion\">去考试</el-button>\n          <el-tag v-else-if=\"scope.row.examStatus===1\" size=\"medium\" type=\"info\">无操作</el-tag>\n          <el-button size=\"mini\" v-else type=\"info\" icon=\"el-icon-view\" @click=\"examQueryGrade(scope.row.userGradeId,scope.row.examStatus)\">查看成绩</el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n    <div style=\"margin: 10px 0\">\n      <el-pagination @size-change=\"handleSizeChange\" @current-change=\"handleCurrentChange\" :current-page=\"currentPage\" :page-sizes=\"[5, 10, 15, 20]\" :page-size=\"pageSize\" layout=\"total, sizes, prev, pager, next, jumper\" :total=\"total\">\n      </el-pagination>\n    </div>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport request from \"@/utils/request\";\nimport { ElMessage } from \"element-plus\";\nimport { Contents } from \"@textbus/core\";\nexport default {\n  name: \"TestPaperList\",\n  components: {},\n  data() {\n    return {\n      options: [],\n      value: \"\",\n      examStatusOptions: [\n        { value: \"\", label: \"请选择\" },\n        { value: 0, label: \"未完成\" },\n        { value: 1, label: \"批改中\" },\n        { value: 2, label: \"已完成\" },\n      ],\n      examStatusValue: \"\",\n      form: {},\n      dialogVisible: false,\n      tableData: [],\n      tableDatas: [],\n      useYnValue: \"\",\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      search: \"\",\n      cloumWidth: {\n        id: \"50\",\n        examName: \"120\",\n        AnswerTime: \"50\",\n        subjectName: \"70\",\n        examScore: \"50\",\n        passingScore: \"50\",\n        grade: \"50\",\n        examHsTime: \"50\",\n        topicNum: \"50\",\n        answerDate: \"100\",\n        markDate: \"170\",\n        examStatus: \"82\",\n        cz: \"180\",\n      },\n    };\n  },\n  created() {\n    this.load();\n    this.defaultTableWith();\n    //初始化试卷类型\n    let phone = sessionStorage.getItem(\"phone\");\n    request.post(\"/exam/createExamDefault\", phone).then((res) => {\n      //初始化类型\n      this.options.push({\n        value: \"\",\n        label: \"请选择\",\n      });\n      for (let i = 0; i < res.date.subjects.length; i++) {\n        this.options.push({\n          value: res.date.subjects[i].id,\n          label: res.date.subjects[i].subjectName,\n        });\n      }\n    });\n  },\n  mounted() {\n    this.$nextTick(() => {\n      window.addEventListener(\"resize\", () => {\n        //监听浏览器窗口大小改变\n        //浏览器变化执行动作\n        //初始化表格宽度\n        this.defaultTableWith();\n      });\n    });\n  },\n  methods: {\n    //初始化表格宽度\n    defaultTableWith() {\n      let width = document.body.clientWidth - 241; //表格宽度\n      let kd = [50, 180, 80, 90, 60, 70, 60, 80, 80, 170, 170, 90];\n      let data = this.cloumWidth;\n      var i = 0;\n      for (var key in data) {\n        this.cloumWidth[key] = parseInt((kd[i] / 1295) * width);\n        i++;\n      }\n    },\n    load() {\n      //试卷类型初始化\n      this.value = \"\";\n      //试卷状态初始化\n      this.examStatusValue = \"\";\n      request\n        .get(\"/exam/getUserExamAll\", {\n          params: {\n            pageNumber: this.currentPage,\n            pageSize: this.pageSize,\n            search: this.search,\n          },\n        })\n        .then((res) => {\n          //判断token是否过期\n          this.isToken(res);\n          this.tableData = res.date.records; //初始化表格数据\n          //初始化时间数据\n          for (let i = 0; i < res.date.records.length; i++) {\n            if (this.tableData[i].examHsTime != null) {\n              this.tableData[i].examHsTime = this.getExamHsTime(\n                this.tableData[i].examHsTime\n              );\n            }\n          }\n          this.total = res.date.total; //初始化总条数\n          this.tableDatas = this.tableData;\n        });\n    },\n    handleSizeChange(pageSize) {\n      //改变当前每页的个数触发\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNumber) {\n      //改变当前页码触发\n      this.currentPage = pageNumber;\n      this.load();\n    },\n    handleEdit(id, examStatus) {\n      this.$router.push(\"/testPaperStu\");\n      sessionStorage.setItem(\n        \"StudentExamId\",\n        this.$encryption(JSON.stringify(id))\n      );\n      sessionStorage.setItem(\"examId\", id);\n      sessionStorage.setItem(\"StudentExamStatus\", examStatus);\n    },\n    examQueryGrade(id) {\n      this.$router.push(\"/testPaperStuQueryGrade\");\n      sessionStorage.setItem(\n        \"StudentQueryGradeInfo\",\n        this.$encryption(JSON.stringify(id))\n      );\n    },\n    isToken(res) {\n      if (res === \"\") {\n        ElMessage.error(\"token已过期,请重新登录!\");\n        //清空token\n        localStorage.removeItem(\"token\");\n        //清除session\n        sessionStorage.removeItem(\"phone\");\n        //跳转登录页面\n        this.$router.push(\"/\");\n      }\n    },\n    //处理时间\n    getExamHsTime(time) {\n      var times = \"\";\n      if (time < 60) {\n        let s = time;\n        if (s < 10) {\n          s = \"0\" + s;\n        }\n        times = \"00:00:\" + s;\n      } else if (time < 3600) {\n        let s = time % 60;\n        let m = (time - s) / 60;\n        if (m < 10) {\n          m = \"0\" + m;\n        }\n        if (s < 10) {\n          s = \"0\" + s;\n        }\n        times = \"00:\" + m + \":\" + s;\n      } else {\n        let s = time % 60;\n        let m = ((time - s) / 60) % 60;\n        let h = parseInt(time / 3600);\n        if (h < 10) {\n          h = \"0\" + h;\n        }\n        if (m < 10) {\n          m = \"0\" + m;\n        }\n        if (s < 10) {\n          s = \"0\" + s;\n        }\n        times = h + \":\" + m + \":\" + s;\n      }\n      return times;\n    },\n    examType() {\n      //试卷状态初始化\n      this.examStatusValue = \"\";\n      //选择试卷类型\n      const data = [];\n      this.tableData = [];\n      this.tableData = this.tableDatas;\n      if (this.value != \"\") {\n        for (let i = 0; i < this.tableData.length; i++) {\n          if (this.tableData[i].subjectId == this.value) {\n            data.push(this.tableData[i]);\n          }\n        }\n        this.tableData = data;\n      }\n    },\n    examStatus() {\n      //试卷类型初始化\n      this.value = \"\";\n      //选择试卷状态\n      const data = [];\n      this.tableData = [];\n      this.tableData = this.tableDatas;\n      if (\n        this.examStatusValue === 0 ||\n        this.examStatusValue === 1 ||\n        this.examStatusValue === 2\n      ) {\n        for (let i = 0; i < this.tableData.length; i++) {\n          if (this.tableData[i].examStatus === this.examStatusValue) {\n            data.push(this.tableData[i]);\n          }\n        }\n        this.tableData = data;\n      }\n    },\n  },\n};\n</script>\n<style scoped>\n.kd {\n  width: 100%;\n}\n</style>"],"sourceRoot":""}]}