AA,CAeM,KAfN,EAeM;AAfD,MAAA,KAAK,EAAC,MAeL;AAfkC,MAAA,GAAG,EAAE,IAAI,CAAC,IAe5C;AAfkD,MAAA,KAA0B,EAA1B;AAAA,mBAAA;AAAA;AAelD,KAfN,E,CACE,YAAA,CAAyC,KAAzC,EAAA,UAAA,EAAyC,gBAAA,CAAlB,IAAI,CAAC,IAAa,CAAzC,EAAgC;AAAA;AAAhC,K,EACA,YAAA,CAWM,KAXN,EAAA,UAAA,EAWM,CAVJ,UAUI,EATkD,IAAI,CAAC,IAAL,IAAS,C,kBAA/D,YAAA,CAA6E,KAA7E,EAAA,UAAA,C,qCASI,EARkD,IAAI,CAAC,IAAL,IAAS,C,kBAA/D,YAAA,CAA6E,KAA7E,EAAA,UAAA,C,qCAQI,EAPkD,IAAI,CAAC,IAAL,IAAS,C,kBAA/D,YAAA,CAA6E,KAA7E,EAAA,UAAA,C,qCAOI,EANJ,YAAA,CAKM,KALN,EAKM,IALN,EAKM,CAJJ,YAAA,CAA4D,GAA5D,EAA4D;AAAxD,MAAA,KAAK,EAAA;AAAA,QAAA,IAAA,EAAU,IAAI,CAAC,IAAL,IAAS;AAAnB;AAAmD,KAA5D,E,iBAAwC,IAAI,CAAC,Q,CAA7C,EAAqD;AAAA;AAArD,KAII,EAHJ,YAAA,CAEI,GAFJ,EAAA,WAAA,EAEI,CADU,IAAI,CAAC,K,kBAAjB,YAAA,CAA+C,MAA/C,EAA+C,WAA/C,EAA+C,gBAAA,CAApB,IAAI,CAAC,KAAe,CAA/C,EAAqC;AAAA;AAArC,K,qCACE,E,iBAD6C,SAAI,gBAAA,CAAG,IAAI,CAAC,UAAR,CAAJ,GAAyB,MAAzB,GAA6B,gBAAA,CAAE,IAAI,CAAC,WAAP,CAA7B,GAAiD,OAAjD,GAAsD,gBAAA,CAAE,IAAI,CAAC,QAAP,C,EAAe;AAAA;KAClH,CAFJ,CAGI,CALN,CAMI,CAXN,C,EAYA,YAAA,CAAuE,KAAvE,EAAA,WAAA,EAAmB,SAAI,gBAAA,CAAG,IAAI,CAAC,KAAR,CAAJ,GAAoB,QAApB,GAA0B,gBAAA,CAAG,IAAI,CAAC,SAAR,CAA7C,EAA8D;AAAA;AAA9D,K,CAdF,C;GAeM,CAfN,E;;AAAA,GAgBI,EAjBN,CAkBI,CAtBN,C","sourcesContent":["<template>\n  <div class=\"main\">\n    <div class=\"title\">\n      <div>总分成绩天梯榜</div>\n    </div>\n    <div class=\"list-box\">\n      <div class=\"list\" v-for=\"item in list\" :key=\"item.rank\" style=\"    padding: 10px;\">\n        <div class=\"number\">{{ item.rank }}</div>\n        <div class=\"user\">\n          <span class=\"head-url\"></span>\n          <img src=\"../assets/image/金牌.png\" class=\"level\" v-if=\"item.rank==1\" alt=\"\" />\n          <img src=\"../assets/image/银牌.png\" class=\"level\" v-if=\"item.rank==2\" alt=\"\" />\n          <img src=\"../assets/image/铜牌.png\" class=\"level\" v-if=\"item.rank==3\" alt=\"\" />\n          <div>\n            <p :class=\"{ good: item.good == 1 }\">{{ item.nickName }}</p>\n            <p class=\"jk-num\">\n              <span v-if=\"item.alias\">{{ item.alias }}</span>试卷量:{{ item.examNumber }} 题量:{{item.topicNumber}} 正确率:{{item.accuracy}}\n            </p>\n          </div>\n        </div>\n        <div class=\"score\">总得分:{{ item.score }} 试卷总分:{{ item.examScore }}</div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport request from \"@/utils/request\";\nimport { ElMessage } from \"element-plus\";\nexport default {\n  data() {\n    return {\n      list: [],\n    };\n  },\n  created() {\n    //获取排名数据\n    this.load();\n  },\n  methods: {\n    load() {\n      request\n        .get(\"/rank/userGetRank\")\n        .then((res) => {\n          //判断token是否过期\n          this.isToken(res);\n          if (res.code == 200) {\n            //将数据存储在集合中\n            this.list = res.date;\n          } else {\n            ElMessage.warning({\n              message: res.msg,\n              type: \"warning\",\n            });\n          }\n        })\n        .catch((res) => {\n          ElMessage.error(\"系统异常,查询失败！\");\n        });\n    },\n    isToken(res) {\n      if (res === \"\") {\n        ElMessage.error(\"token已过期,请重新登录!\");\n        //清空token\n        localStorage.removeItem(\"token\");\n        //清除session\n        sessionStorage.removeItem(\"phone\");\n        //跳转登录页面\n        this.$router.push(\"/\");\n      }\n    },\n  },\n};\n</script>\n<style>\n* {\n  margin: 0;\n  padding: 0;\n}\n\n.main {\n  width: 100%;\n  overflow: hidden;\n  padding: 20px;\n  box-sizing: border-box;\n  color: rgb(0 0 0 / 80%);\n}\n\n.main .title {\n  width: 100%;\n  text-align: center;\n  font-size: 20px;\n  font-weight: bold;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.main .title img {\n  width: 50px;\n  height: 50px;\n  margin-right: 10px;\n}\n\n.main .list-box {\n  margin: 25px 0;\n  background-color: rgb(229 234 239);\n  border-radius: 16px;\n}\n\n.main .list-box .list {\n  display: flex;\n  align-items: center;\n  padding: 20px 10px;\n  color: rgb(0 0 0 / 80%);\n  box-sizing: border-box;\n}\n\n.main .list-box .list .number {\n  width: 25px;\n  text-align: left;\n  color: rgb(0 0 0 / 80%);\n  font-size: 17px;\n}\n\n.main .list-box .list .user {\n  display: flex;\n  align-items: center;\n  position: relative;\n}\n\n.main .list-box .list .user .head-url {\n  margin-right: 50px;\n}\n\n.main .list-box .list .user .level {\n  display: block;\n  position: absolute;\n  top: -3.5px;\n  left: -7.5px;\n  width: 50px;\n  z-index: 10;\n}\n\n.main .list-box .list .user .good {\n  font-weight: bold;\n}\n\n.main .list-box .list .user div {\n  min-width: 0;\n  flex: 1;\n}\n\n.main .list-box .list .user div .jk-num {\n  margin-top: 10px;\n  font-size: 10px;\n  color: rgb(32 149 63);\n}\n\n.main .list-box .list .user div span {\n  margin-right: 10px;\n  color: rgb(238, 76, 76);\n}\n\n.main .list-box .list .score {\n  margin-left: auto;\n  font-size: 14px;\n}\n</style>"],"sourceRoot":""}]}